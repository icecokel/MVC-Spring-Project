<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="accountmapper">
	<select id="server" resultType="IceServer" parameterType="java.lang.Integer" >
	 select num, serverid,serverpw,port,host
	 from server 
	 where num =#{num}
	
	</select>
</mapper> 